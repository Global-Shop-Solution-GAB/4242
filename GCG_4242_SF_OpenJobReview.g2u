F.Intrinsic.Control.SetErrorHandler("cmdWOSO_Click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
v.Local.stitles.declare(String)
v.Local.iwidths.Declare(long)
v.Local.sret.Declare(String)
v.Local.ssql.Declare(String)
v.Local.sorderno.Declare(string)
v.Local.sparam.Declare(String)

'This part was added Valentines day 2017. Customer was under the impression the program would allow him to schedule each line of a sales order. 
'We added funcionality from an older script to allow them to select a sales order than call callwrapper 200800
'display a browser for sales orders to select one
Function.Intrinsic.String.Split("Order No*!*Customer*!*Date Order","*!*",variable.local.sTitles)
Function.Intrinsic.String.Split("1000*!*1200*!*800","*!*",variable.local.iWidths)
Function.Intrinsic.UI.Browser("Select a job","con","Select ORDER_NO, CUSTOMER, DATE_ORDER From V_ORDER_HEADER",Variable.Local.sTitles,Variable.Local.iWidths,Variable.Local.sRet)
Function.Intrinsic.Control.If(Variable.Local.sRet,"=","***CANCEL***")
 	Function.Intrinsic.UI.Msgbox("No order was selected!")
	f.Intrinsic.Control.ExitSub
Function.Intrinsic.Control.Else
	f.Intrinsic.UI.InvokeWaitDialog("Generating Work Orders...")
	'split the return and launch the callwrapper
	 Function.Intrinsic.String.Split(Variable.Local.sRet,"!*!",Variable.Local.sRet)
	v.Local.sorderno.Set(v.Local.sret(0))
	f.Intrinsic.String.Build("{0}!*!{1}!*!{2}","O",v.Local.sorderno,"NS",v.Local.sparam)
	F.Global.General.CallWrapperSync(200800,V.Local.sParam)
	f.Intrinsic.UI.CloseWaitDialog
Function.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdWOSO_Click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_4242_SF_OpenJobReview.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	f.Intrinsic.Control.CallSub(unload)
Function.Intrinsic.Control.EndIf


Program.Sub.cmdWOSO_Click.End

Program.Sub.Comments.Start
${$0$}$GAB_4169_SF_OpenJobReview_GSS_Standard.g2u$}$Joey & AGV$}$7/18/2016 9:03:10 AM$}$False
${$3$}$0$}$$}$0$}$-1$}$$}$12:00:00 AM$}$This will be a custom GAB script that will run from a custom Menu Item under Bill of Material -> File -> Custom Cobalt BOM Import.  The user will navigate to the xml file that needs to be loaded into Global Shop.  Once the xml is parsed there will be a search in Global Shop to see if a BOM already exists for that Part Number.  If one already exists a compare screen will display showing the differences between the two.  If one does not exist then the screen will display the BOM as it will be uploaded into Global Shop.  Any parts that are listed in the BOM that are not Inventory Parts will be highlighted in Yellow.  If there are any parts not in inventory the user will have the option to click the create Parts button to generate those parts in Global Shop.  If the BOM exists in Global shop then any differences on the new import will be highlighted in Red.  After the Parts have been created then the Upload button will upload and override any previous BOM for that part.
Program.Sub.Comments.End

